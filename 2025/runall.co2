
before = clock();

loop (i=1, i<=7, i=i+1)
{
    view("------------------------------\nRunning: day", i, "from 2025");
    count = string(i);
    if (length(count) < 2) count = "0" + count;
    result = system("co2 library.co2 day"+count+".co2 framework.co2");
    if (result != 0) {
        view("FAILED RUN!");
        exit(1);
    }
};

view("runall completed in", (clock()-before)/1000000, "ms");
