


test = function(case, index){
	local in = get(case, "in");
	local out = get(case, "out");
    local time = clock();
	local result2 = solution(in);
    time = clock() - time;
	if (equals(result2, out)){
		view("case", index, "passed with", result2, "(" + string(time/1000000) + "ms)");
	} else {
		view("case", index, "failed with", result2, "while expecting", out);
	};
};

apply(test, cases);